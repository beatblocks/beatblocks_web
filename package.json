{
  "name": "beatblocks_web",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebookincubator/create-react-app).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,README.md,./npm-debug.log,./client --copy-files",
    "start": "npm run build && nodemon dist/server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run start\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beatblocks/beatblocks_web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beatblocks/beatblocks_web/issues"
  },
  "homepage": "https://github.com/beatblocks/beatblocks_web#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "ipfs-api": "^20.0.1",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "web3": "^1.0.0-beta.26"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-rallycoding": "^3.2.0",
    "rimraf": "^2.6.2"
  }
}
